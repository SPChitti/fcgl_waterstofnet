{
  "distance_influence": 70,
  "priority": [
    {
      "if": "average_slope > 8",
      "multiply_by": 0.4
    },
    {
      "else_if": "average_slope > 5",
      "multiply_by": 0.7
    },
    {
      "else_if": "average_slope < -5",
      "multiply_by": 1.05
    },
    {
      "else_if": "road_class == RESIDENTIAL || road_class == LIVING_STREET",
      "multiply_by": 0.85
    },
    {
      "else_if": "road_access == DESTINATION",
      "multiply_by": 0.1
    },
    {
      "else_if": "road_class == MOTORWAY",
      "multiply_by": 1.15
    },
    {
      "else": "",
      "multiply_by": 1.0
    }
  ],
  "speed": [
    {
      "if": "average_slope > 10",
      "limit_to": 55
    },
    {
      "else_if": "average_slope > 6",
      "limit_to": 70
    },
    {
      "else_if": "road_class == MOTORWAY",
      "limit_to": 85
    },
    {
      "else_if": "road_class == PRIMARY",
      "limit_to": 75
    },
    {
      "else": "",
      "limit_to": 50
    }
  ]
}