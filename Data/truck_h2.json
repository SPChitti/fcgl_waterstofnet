{
  "distance_influence": 70,
  "priority": [
    {
      "if": "average_slope > 12",
      "multiply_by": 0.97
    },
    {
      "else_if": "road_class == MOTORWAY",
      "multiply_by": 1.2
    },
    {
      "else_if": "road_class == PRIMARY || road_class == SECONDARY",
      "multiply_by": 1.1
    },
    {
      "else_if": "road_environment == TUNNEL",
      "multiply_by": 0.9
    },
    {
      "else_if": "road_class == RESIDENTIAL || road_class == LIVING_STREET",
      "multiply_by": 0.95
    },
    {
      "else_if": "road_access == DESTINATION",
      "multiply_by": 0.1
    },
    {
      "else": "",
      "multiply_by": 1.0
    }
  ],
  "speed": [
    {
      "if": "road_class == MOTORWAY",
      "limit_to": 90
    },
    {
      "else_if": "road_class == PRIMARY",
      "limit_to": 80
    },
    {
      "else_if": "road_class == SECONDARY",
      "limit_to": 70
    },
    {
      "else": "",
      "limit_to": 50
    }
  ]
}